header#page-header
	include off-canvas
	nav.navbar.fixed-top.navbar-toggleable-md.warning-color-dark.navbar-light.zindex-sm.scrolling-navbar.double-nav
		.float-xs-left
			a.button-collapse(href='#slide-out' data-toggle='collapse' aria-expanded="false" aria-controls="slide-out")
				i.fa.fa-bars
		.breadcrumb-dn.mr-auto
			p Material Design for Bootstrap
		ul.nav.navbar-nav.ml-auto.flex-row
			li.nav-item
				a.nav-link.waves-effect.waves-light
					i.fa.fa-envelope
					span.hidden-sm-down Contact
			li.nav-item
				a.nav-link.waves-effect.waves-light
					i.fa.fa-comments-o
					span.hidden-sm-down Support
			li.nav-item
				a.nav-link.waves-effect.waves-light
					i.fa.fa-user
					span.hidden-sm-down Account
			li.nav-item.dropdown
				a#navbarDropdownMenuLink.nav-link.dropdown-toggle.waves-effect.waves-light(href='#', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
					| Dropdown
				.dropdown-menu.dropdown-menu-right(aria-labelledby='navbarDropdownMenuLink')
					a.dropdown-item.waves-effect.waves-light(href='#') Action
					a.dropdown-item.waves-effect.waves-light(href='#') Another action
					a.dropdown-item.waves-effect.waves-light(href='#') Something else here

