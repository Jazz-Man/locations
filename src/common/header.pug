		.container
			button.navbar-toggler.navbar-toggler-right(type='button' data-toggle='collapse' data-target='#navbarNav' aria-controls='navbarNav' aria-expanded='false' aria-label='Toggle navigation')
				span.navbar-toggler-icon
			a.navbar-brand(href='#')
				img.d-inline-block.align-top(src=require('assets/img/logo.png'))
			.collapse.navbar-collapse#navbarNav
				ul.navbar-nav.mr-auto
					li.nav-item.active
						+link('/index.html','Home')(class="nav-link")
					li.nav-item
						+link('/map.html','Map')(class="nav-link")
					li.nav-item
						+link('/blog.html','Blog')(class="nav-link page-loader-ignore")
					li.nav-item
						+link('/register.html','Register')(class="nav-link page-loader-ignore")
					li.nav-item
						+link('/sign-in.html','Sign In')(class="nav-link page-loader-ignore")

				ul.navbar-nav.ml-auto.nav-flex-icons
					li.nav-item
						a.nav-link.waves-effect.waves-light
							| 1
							i.fa.fa-envelope
					li.nav-item.avatar.dropdown
						a#navbarDropdownMenuLink.nav-link.dropdown-toggle.waves-effect.waves-light(href='#slide-out' role="button" data-toggle='collapse' aria-controls="slide-out" aria-expanded='false')
							img.img-fluid.rounded-circle.z-depth-0(src='http://mdbootstrap.com/img/Photos/Avatars/avatar-2.jpg')
						.dropdown-menu.dropdown-warning.dropdown-menu-right(aria-labelledby='navbarDropdownMenuLink')
							a.dropdown-item.waves-effect.waves-light(href='#') Action
							a.dropdown-item.waves-effect.waves-light(href='#') Another action
							a.dropdown-item.waves-effect.waves-light(href='#') Something else here