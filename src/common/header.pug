header#page-header
	nav.navbar.navbar-toggleable-md.warning-color-dark.zindex-sm
		.container
			button.navbar-toggler.navbar-toggler-right(type='button' data-toggle='collapse' data-target='#navbarNav' aria-controls='navbarNav' aria-expanded='false' aria-label='Toggle navigation')
				span.navbar-toggler-icon
			a.navbar-brand(href='#')
				img.d-inline-block.align-top(src=require('assets/img/logo.png'))
			.collapse.navbar-collapse#navbarNav
				ul.navbar-nav.mr-auto
					li.nav-item.active
						+link('/index.html','Home')(class="nav-link black-text")
					li.nav-item
						+link('/map.html','Map')(class="nav-link black-text")
					//li.nav-item
					//	+link('/blog.html','Blog')(class="nav-link black-text")
					//li.nav-item
					//	+link('/contact.html','Contact')(class="nav-link black-text")
				ul.nav.navbar-nav.nav-flex-icons
					li.nav-item
						a.nav-link.waves-effect.waves-light
							i.fa.fa-facebook
					li.nav-item
						a.nav-link.waves-effect.waves-light
							i.fa.fa-twitter
					li.nav-item
						a.nav-link.waves-effect.waves-light
							i.fa.fa-instagram
